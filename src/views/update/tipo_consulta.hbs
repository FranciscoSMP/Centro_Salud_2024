<h2 class="text-center my-4">Actualizar Tipo de Consulta</h2>
<form action="/tipo_consulta/actualizar" method="POST" class="container form-General">
    <input type="hidden" name="Id_Consulta" value="{{tipo_consulta.Id_Consulta}}" />
    <div class="form-group mb-3">
        <label for="Fecha_Consulta" class="form-label">Fecha de Consulta</label>
        <input type="date" name="Fecha_Consulta" id="Fecha_Consulta" class="form-control"
            value="{{tipo_consulta.Fecha_Consulta}}" required>
        <p></p>
        <label for="Tipo_Consulta" class="form-label">Tipo de Consulta <font color="red">*</font></label>
        <input type="text" name="Tipo_Consulta" id="Tipo_Consulta" class="form-control" 
        value="{{tipo_consulta.Tipo_Consulta}}">
    </div>
    </div>
    <div class="text-center">
        <button type="submit" class="btn btn-primary">Actualizar</button>
        <a href="/tipo_consulta/table" class="btn btn-secondary">Cancelar</a>
    </div>
</form>