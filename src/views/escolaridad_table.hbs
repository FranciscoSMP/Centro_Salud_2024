<h2>Lista de Escolaridades</h2>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Nivel de Escolaridad</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each escolaridad}}
        <tr>
            <td>{{this.Id_Escolaridad}}</td> <!-- AsegÃºrate de que los campos coincidan con la base de datos -->
            <td>{{this.Nivel_Escolaridad}}</td>
            <td>
                <a href="/escolaridad/editar/{{this.Id_Escolaridad}}">Editar</a>
            </td>
        </tr>
        {{/each}}
    </tbody>
</table>
